pipeline {
    agent any
    tools {
            maven 'Maven'
            jdk 'JDK11'
    }
    stages {
        stage('validate') {
            steps {
                echo 'VALIDATE'
                sh 'mvn clean validate'
            }
        }
        stage('Compile') {
            steps {
                echo 'COMPILE'
                sh 'mvn clean compile'
            }
        }
    }
}

