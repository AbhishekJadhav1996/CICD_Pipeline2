pipeline {
    agent any
    tools {
            maven 'Maven'
    }
    stages {
        stage('validate') {
            steps {
                echo 'VALIDATE'
                sh 'mvn validate'
            }
        }
        stage('Compile') {
            steps {
                echo 'COMPILE'
                sh 'mvn compile'
            }
        }
    }
}

